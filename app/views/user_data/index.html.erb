<p id="notice"><%= notice %></p>

<h1>User Data</h1>

<table>
  <thead>
    <tr>
      <th>Email</th>
      <th>Username</th>
      <th>Encrypted pass</th>
      <th>Salt</th>
      <th>Phone</th>
      <th>Created on</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_data.each do |user_datum| %>
      <tr>
        <td><%= user_datum.email %></td>
        <td><%= user_datum.username %></td>
        <td><%= user_datum.encrypted_pass %></td>
        <td><%= user_datum.salt %></td>
        <td><%= user_datum.phone %></td>
        <td><%= user_datum.created_on %></td>
        <td><%= link_to 'Show', user_datum %></td>
        <td><%= link_to 'Edit', edit_user_datum_path(user_datum) %></td>
        <td><%= link_to 'Destroy', user_datum, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User Datum', new_user_datum_path %>
